{
  // Python settings
  "python.pythonPath": "/usr/local/bin/python3",
  "editor.fontLigatures": true,
  "code-runner.executorMap": {
    "javascript": "node",
    "java": "cd $dir && javac $fileName && java $fileNameWithoutExt",
    "c": "cd $dir && gcc $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "cpp": "cd $dir && g++ $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "objective-c": "cd $dir && gcc -framework Cocoa $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "php": "php",
    "python": "/usr/local/bin/python3 -u",
    "perl": "perl",
    "perl6": "perl6",
    "ruby": "ruby",
    "go": "go run",
    "lua": "lua",
    "groovy": "groovy",
    "powershell": "powershell -ExecutionPolicy ByPass -File",
    "bat": "cmd /c",
    "shellscript": "bash",
    "fsharp": "fsi",
    "csharp": "scriptcs",
    "vbscript": "cscript //Nologo",
    "typescript": "ts-node",
    "coffeescript": "coffee",
    "scala": "scala",
    "swift": "swift",
    "julia": "julia",
    "crystal": "crystal",
    "ocaml": "ocaml",
    "r": "Rscript",
    "applescript": "osascript",
    "clojure": "lein exec",
    "haxe": "haxe --cwd $dirWithoutTrailingSlash --run $fileNameWithoutExt",
    "rust": "cd $dir && rustc $fileName && $dir$fileNameWithoutExt",
    "racket": "racket",
    "ahk": "autohotkey",
    "autoit": "autoit3",
    "dart": "dart",
    "pascal": "cd $dir && fpc $fileName && $dir$fileNameWithoutExt",
    "d": "cd $dir && dmd $fileName && $dir$fileNameWithoutExt",
    "haskell": "runhaskell",
    "nim": "nim compile --verbosity:0 --hints:off --run"
  },
  "editor.letterSpacing": 0.05,
  "window.zoomLevel": 1,
  "editor.fontSize": 15,
  "editor.fontWeight": "400",
  "editor.wordWrap": "on",
  "editor.lineNumbers": "relative",
  "editor.fontFamily": "Iosevka",
  "editor.lineHeight": 20,
  // Vim settings
  "editor.cursorBlinking": "solid",
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.overrideCopy": false,
  "vim.autoindent": true,
  "vim.useCtrlKeys": true,
  "vim.enableNeovim": true,
  "vim.hlsearch": true,
  //"vim.foldfix": true,
  "vim.insertModeKeyBindings": [
    {
      "before": [ "k", "j" ],
      "after": [ "<Esc>" ]
    }
  ],
  "vim.leader": "<space>",
  "vim.visualModeKeyBindings": [
    {
      "before": [ "<cr>" ],
      "after": ["g", "b"]
    }
  ],
  "vim.normalModeKeyBindings": [
    {
      "before": [ "<cr>" ],
      "after": ["g", "b"]
    },
    {
      "before": [ "leader", "/" ],
      "after": [],
      "commands": [ { "command": ":nohl" } ]
    },
    {
      "before": [ "leader", "." ],
      "after": [],
      "commands": [ { "command": "workbench.action.showAllSymbols" } ]
    },
    {
      "before": [ "leader", "l", "r" ],
      "after": [],
      "commands": [ { "command": "editor.action.rename" } ]
    },
    {
      "before": [ "leader", "l", "a" ],
      "after": [],
      "commands": [ { "command": "editor.action.quickFix" } ]
    },
    {
      "before": [ "leader", "f" ],
      "after": [],
      "commands": [ { "command": "workbench.action.findInFiles" } ]
    },
    {
      "before": [ "leader", "w" ],
      "after": [],
      "commands": [ { "command": ":w" } ]
    },
    {
      "before": [ "leader", "q" ],
      "after": [],
      "commands": [ { "command": ":q" } ]
    },
    {
      "before": [ "leader", "m" ],
      "after": [],
      "commands": [ { "command": "editor.action.formatDocument" } ]
    },
    {
      "before": [ "leader", "p" ],
      "after": [],
      "commands": [ { "command": "workbench.action.quickOpen" } ]
    },
    {
      "before": [ "leader", "n" ],
      "after": [],
      "commands": [ { "command": "workbench.action.toggleSidebarVisibility" } ]
    },
    {
      "before": [ "leader", "c", "c" ],
      "after": [ "g", "c", "c" ],
      "commands": []
    },
    {
      "before": [ "<C-d>" ],
      "after": ["1", "6", "j"]
    },
    {
      "before": [ "<C-u>" ],
      "after": ["1", "6", "k"]
    },
    {
      "before": [ "<right>" ],
      "after": [],
      "commands": [ { "command": "workbench.action.nextEditor" } ]
    },
    {
      "before": [ "<left>" ],
      "after": [],
      "commands": [ { "command": "workbench.action.previousEditor" } ]
    },
    {
      "before": [ "Y" ],
      "after": [ "y", "$" ],
      "commands": []
    },
    {
      "before": [ "z", "v" ],
      "after": [ "z", "o" ]
    },
    {
      "before": [ "U" ],
      "after": [ "<C-r>" ]
    },
    {
      "before": [ "leader", "w" ],
      "after": [],
      "commands": [ { "command": "workbench.action.files.save", "args": [] } ]
    },
    {
      "before": [ "leader", "r" ],
      "after": [],
      "commands": [ { "command": "code-runner.run", "args": [] } ]
    },
    {
      "before": [ ";" ],
      "after": [ ":" ]
    },
    {
      "before": [ "L" ],
      "after": [ "$" ]
    },
    {
      "before": [ "H" ],
      "after": [ "^" ]
    }
  ],
  "workbench.sideBar.location": "right",
  "workbench.statusBar.visible": true,
  "workbench.colorCustomizations": {
    "editorCursor.background": "#062029",
    "editorCursor.foreground": "#839496",
  },
  "files.exclude": {
    "**/__pycache__": true,
    "**/.class": true,
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.svn": true,
    "**/CVS": true,
    "**/venv": true
  },
  "workbench.activityBar.visible": false,
  "editor.renderWhitespace": "none",
  "editor.renderControlCharacters": false,
  "workbench.startupEditor": "newUntitledFile",
  "vetur.format.defaultFormatter.css": "prettier",
  "vetur.format.defaultFormatter.js": "vscode-typescript",
  "vetur.format.defaultFormatter.html": "prettyhtml",
  "prettier.eslintIntegration": true,
  "explorer.openEditors.visible": 0,
  "editor.minimap.enabled": false,
  "editor.acceptSuggestionOnEnter": "off",
  "explorer.confirmDelete": false,
  "latex-workshop.chktex.enabled": false,
  "latex-workshop.view.pdf.viewer": "browser",
  "workbench.statusBar.feedback.visible": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "java.home": "/Library/Java/JavaVirtualMachines/jdk1.8.0_172.jdk/Contents/Home",
  "workbench.editor.enablePreview": false,
  "cSpell.language": "en-GB",
  "cSpell.userWords": [
    "pseudocode"
  ],
  "python.linting.flake8Enabled": true,
  "python.linting.pylintEnabled": false,
  "python.linting.flake8Args": [
    "--ignore=E114,E116,E131,E722",
    "--max-line-length=120"
  ],
  "languageServerHaskell.hieExecutablePath": "hie-wrapper",
  "workbench.colorTheme": "Solarized Dark",
  "workbench.iconTheme": "material-icon-theme",
  "search.runInExtensionHost": true,
  "todohighlight.isEnable": true,
  "todohighlight.keywords": [
     "TODO",
     "FIXME",
     "NOTE"
    ],
  "todohighlight.include": [
    "**/*.js",
    "**/*.jsx",
    "**/*.c",
    "**/*.ts",
    "**/*.tsx",
    "**/*.html",
    "**/*.php",
    "**/*.css",
    "**/*.scss"
  ]
}
